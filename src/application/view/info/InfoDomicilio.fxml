<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER_LEFT" layoutX="19.0" layoutY="17.0" prefHeight="383.0" prefWidth="581.0" spacing="20.0">
         <children>
            <Label text="Domicilio" />
            <TableView pickOnBounds="false" prefHeight="233.0" prefWidth="600.0">
              <columns>
                <TableColumn prefWidth="75.0" text="id" />
                  <TableColumn prefWidth="75.0" text="Calle" />
                  <TableColumn prefWidth="75.0" text="Numero" />
                  <TableColumn prefWidth="75.0" text="Localidad" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <HBox alignment="CENTER" prefHeight="83.0" prefWidth="600.0" spacing="20.0">
               <children>
                  <Button alignment="CENTER" mnemonicParsing="false" onAction="#handleNewEmpleado" text="Nuevo Domicilio" />
                  <Button alignment="CENTER" mnemonicParsing="false" onAction="#handleEditPerson" text="Editar Domiclio" />
                  <Button alignment="CENTER" mnemonicParsing="false" onAction="#handleDeleteEmpleado" text="Eliminar Domicilio" />
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
